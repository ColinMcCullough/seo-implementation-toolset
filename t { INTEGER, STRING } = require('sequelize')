[32m+[m[32masync function createLocations (lpId, locations) {[m
[32m+[m[32m  const returnLocations = [][m
[32m+[m[32m  for (let i = 0; i < locations.length; i++) {[m
[32m+[m[32m    const { name, properties } = locations[i][m
[32m+[m[32m    const location = await models.location.create({[m
[32m+[m[32m      lpId,[m
[32m+[m[32m      name,[m
[32m+[m[32m      properties[m
[32m+[m[32m    })[m
[32m+[m[32m    returnLocations.push(location)[m
[32m+[m[32m  }[m
[32m+[m[32m  return returnLocations[m
[32m+[m[32m}[m
