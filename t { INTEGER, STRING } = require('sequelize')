[32m+[m
[32m+[m[32mconst minTest = parseInt(min)[m
[32m+[m[32mconst maxTest = parseInt(max)[m
[32m+[m[32mconst idleTest = parseInt(idle)[m
[32m+[m[32mconst acquireTest = parseInt(acquire)[m
[32m+[m[32mconst evictTest = parseInt(evict)[m
[32m+[m
[32m+[m[32mconst sequelize = new Sequelize(dbUrl, {[m
[32m+[m[32m  pool: { max: maxTest, min: minTest, idle: idleTest, acquire: acquireTest, evict: evictTest },[m
