[32m+[m[32m      hasMsg: false,[m
[32m+[m[32m      msg: '',[m
[32m+[m[32m      alertvariant: ''[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  computed: {[m
[32m+[m[32m    form: {[m
[32m+[m[32m      get() {[m
[32m+[m[32m        return {[m
[32m+[m[32m          neighborhood: this.location.properties.neighborhood,[m
[32m+[m[32m          neighborhood_2: this.location.properties.neighborhood_2,[m
[32m+[m[32m          landmark_1_name: this.location.properties.landmark_1_name,[m
[32m+[m[32m          apartment_amenity_1: this.location.properties.apartment_amenity_1,[m
[32m+[m[32m          community_amenity_1: this.location.properties.community_amenity_1[m
[32m+[m[32m        }[m
[32m+[m[32m      },[m
[32m+[m[32m      set(val) {}[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  methods: {[m
[32m+[m[32m    validateStepOne() {[m
[32m+[m[32m      let valid = true[m
[32m+[m[32m      for (const prop in this.form) {[m
[32m+[m[32m        if (this.form[prop] === '' || this.form[prop] === null) {[m
[32m+[m[32m          valid = false[m
[32m+[m[32m          break[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      return valid[m
[32m+[m[32m    },[m
[32m+[m[32m    showMsg(msg) {[m
[32m+[m[32m      this.msg = msg[m
[32m+[m[32m      this.alertvariant = 'danger'[m
[32m+[m[32m      this.hasMsg = true[m
[32m+[m[32m    },[m
[32m+[m[32m    onSave() {[m
[32m+[m[32m      this.hasMsg = false[m
[32m+[m[32m      const validFields = this.validateStepOne()[m
[32m+[m[32m      if (validFields) {[m
[32m+[m[32m        this.$emit('step-1-save', { id: this.location.id })[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.showMsg('Please ensure all fields are filled out')[m
[32m+[m[32m        this.$emit('update-step-status', 'stepOneComplete', false)[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    onInput(key, val) {[m
[32m+[m[32m      this.$emit('step-update', { key, val, id: this.location.id })[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
