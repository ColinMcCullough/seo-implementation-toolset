[32m+[m[32masync function run(locationProperties) {[m
[32m+[m[32m  const latAndLong = await getLatLong(locationProperties)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32masync function getLatLong(prop) {[m
[32m+[m[32m  const { address, city, state, zip } = prop[m
[32m+[m[32m  const reqUrl = `https://maps.googleapis.com/maps/api/geocode/json?address=${address}${city}${state}${zip}&key=${key}`[m
[32m+[m[32m  const addrresponse = await axios.get(reqUrl)[m
[32m+[m[32m  const addrdata = JSON.parse(addrresponse)[m
[32m+[m[32m  return { lat: addrdata.results[0].geometry.location.lat,[m
[32m+[m[32m    lon: addrdata.results[0].geometry.location.lng }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction removeDiacritics(str) {[m
[32m+[m[32m  diacriticsRemovalMap.forEach((regexFltr) => {[m
[32m+[m[32m    str = str.replace(regexFltr.letters, regexFltr.base)[m
[32m+[m[32m  })[m
[32m+[m[32m  return str[m
[32m+[m[32m}[m
