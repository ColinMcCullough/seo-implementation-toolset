[32m+[m[32m# PLUGINS[m
[32m+[m
[32m+[m[32m**This directory is not required, you can delete it if you don't want to use it.**[m
[32m+[m
[32m+[m[32mThis directory contains Javascript plugins that you want to run before mounting the root Vue.js application.[m
[32m+[m
[32m+[m[32mMore information about the usage of this directory in [the documentation](https://nuxtjs.org/guide/plugins).[m
[1mdiff --git a/server/index.js b/server/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..98d2d23[m
[1m--- /dev/null[m
[1m+++ b/server/index.js[m
[36m@@ -0,0 +1,34 @@[m
[32m+[m[32mconst express = require('express')[m
[32m+[m[32mconst consola = require('consola')[m
[32m+[m[32mconst { Nuxt, Builder } = require('nuxt')[m
[32m+[m[32mconst app = express()[m
[32m+[m
[32m+[m[32m// Import and Set Nuxt.js options[m
[32m+[m[32mconst config = require('../nuxt.config.js')[m
[32m+[m[32mconfig.dev = process.env.NODE_ENV !== 'production'[m
[32m+[m
[32m+[m[32masync function start () {[m
[32m+[m[32m  // Init Nuxt.js[m
[32m+[m[32m  const nuxt = new Nuxt(config)[m
[32m+[m
[32m+[m[32m  const { host, port } = nuxt.options.server[m
[32m+[m
[32m+[m[32m  // Build only in dev mode[m
[32m+[m[32m  if (config.dev) {[m
[32m+[m[32m    const builder = new Builder(nuxt)[m
[32m+[m[32m    await builder.build()[m
[32m+[m[32m  } else {[m
[32m+[m[32m    await nuxt.ready()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Give nuxt middleware to express[m
[32m+[m[32m  app.use(nuxt.render)[m
[32m+[m
[32m+[m[32m  // Listen the server[m
[32m+[m[32m  app.listen(port, host)[m
[32m+[m[32m  consola.ready({[m
[32m+[m[32m    message: `Server listening on http://${host}:${port}`,[m
[32m+[m[32m    badge: true[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m[32mstart()[m
