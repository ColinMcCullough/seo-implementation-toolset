[32m+[m
[32m+[m[32m<script>[m
[32m+[m[32mimport moment from 'moment'[m
[32m+[m[32mimport CommentsMixin from '~/mixins/comments'[m
[32m+[m[32mimport SaveStep from '~/mixins/savestep'[m
[32m+[m[32mexport default {[m
[32m+[m[32m  mixins: [CommentsMixin, SaveStep],[m
[32m+[m[32m  props: {[m
[32m+[m[32m    location: {[m
[32m+[m[32m      type: Object,[m
[32m+[m[32m      default() {[m
[32m+[m[32m        return {}[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    locationNotes: {[m
[32m+[m[32m      type: Array,[m
[32m+[m[32m      default() {[m
[32m+[m[32m        return [][m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    projectNotes: {[m
[32m+[m[32m      type: Array,[m
[32m+[m[32m      default() {[m
[32m+[m[32m        return [][m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    projectNoteField: {[m
[32m+[m[32m      type: String,[m
[32m+[m[32m      default() {[m
[32m+[m[32m        return ''[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  data () {[m
[32m+[m[32m    return {[m
[32m+[m[32m      saveTxt: 'Save',[m
[32m+[m[32m      toggle: {[m
[32m+[m[32m        selected: 'project'[m
[32m+[m[32m      },[m
[32m+[m[32m      notestbl: {[m
[32m+[m[32m        fields: [[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'author',[m
[32m+[m[32m            label: 'Name'[m
[32m+[m[32m          },[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'updatedAt',[m
[32m+[m[32m            label: 'Date'[m
[32m+[m[32m          },[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'text',[m
[32m+[m[32m            label: 'Comment'[m
[32m+[m[32m          }[m
[32m+[m[32m        ][m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  computed: {[m
[32m+[m[32m    inputTxt: {[m
[32m+[m[32m      get() {[m
[32m+[m[32m        return this.toggle.selected === 'location'[m
[32m+[m[32m          ? this.location.properties.locationNote[m
[32m+[m[32m          : this.projectNoteField[m
[32m+[m[32m      },[m
[32m+[m[32m      set(val) {[m
[32m+[m[32m        this.onLocationTab()[m
[32m+[m[32m          ? this.$emit('stepper-updated', { key: 'locationNote', val, id: this.location.id })[m
[32m+[m[32m          : this.$emit('project-data-input', { key: 'projectNoteField', val })[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    getNotes() {[m
[32m+[m[32m      return this.toggle.selected === 'location'[m
[32m+[m[32m        ? this.locationNotes[m
[32m+[m[32m        : this.projectNotes[m
[32m+[m[32m    },[m
[32m+[m[32m    getOptions() {[m
[32m+[m[32m      return [[m
[32m+[m[32m        { text: 'Project', value: 'project' },[m
[32m+[m[32m        { text: 'Location', value: 'location', disabled: this.location === null }[m
[32m+[m[32m      ][m
[32m+[m[32m    },[m
[32m+[m[32m    disabledSubmit() {[m
[32m+[m[32m      return !this.getTxt()[m
[32m+[m[32m    },[m
[32m+[m[32m    submitTip() {[m
[32m+[m[32m      return !this.getTxt() ? 'submit-tip' : 'not-disabled'[m
[32m+[m[32m    },[m
[32m+[m[32m    placeholderTxt() {[m
[32m+[m[32m      return this.toggle.selected === 'location'[m
[32m+[m[32m        ? `Enter a Location Note for ${this.location.name}`[m
[32m+[m[32m        : 'Enter a Project Note'[m
[32m+[m[32m    },[m
[32m+[m[32m    getItems() {[m
[32m+[m[32m      return this.toggle.selected === 'location' ? this.locationNotes : this.projectNotes[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  methods: {[m
[32m+[m[32m    getTxt() {[m
[32m+[m[32m      return this.onLocationTab() ? this.location.properties.locationNote : this.projectNoteField[m
[32m+[m[32m    },[m
[32m+[m[32m    onInput(val) {[m
[32m+[m[32m      this.inputTxt = val[m
[32m+[m[32m    },[m
[32m+[m[32m    formattedDate(date) {[m
[32m+[m[32m      // eslint-disable-next-line no-console[m
[32m+[m[32m      console.log(date)[m
[32m+[m[32m      return moment(new Date(date)).format('MM-DD-YY')[m
[32m+[m[32m    },[m
[32m+[m[32m    onLocationTab() {[m
[32m+[m[32m      return this.toggle.selected === 'location'[m
[32m+[m[32m    },[m
[32m+[m[32m    onSubmit() {[m
[32m+[m[32m      this.$emit('submit-note', this.toggle.selected)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
