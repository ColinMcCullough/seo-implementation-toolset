[32m+[m[32mimport Papa from 'papaparse'[m
[32m+[m[32mexport default {[m
[32m+[m[32m  methods: {[m
[32m+[m[32m    getAddPropFields() {[m
[32m+[m[32m      return {[m
[32m+[m[32m        population: null,[m
[32m+[m[32m        uspsvalid: null,[m
[32m+[m[32m        recommended_name: null,[m
[32m+[m[32m        redirects: {[m
[32m+[m[32m          fields: [[m
[32m+[m[32m            {[m
[32m+[m[32m              key: 'strategy',[m
[32m+[m[32m              label: 'Strategy'[m
[32m+[m[32m            },[m
[32m+[m[32m            {[m
[32m+[m[32m              key: 'current_url',[m
[32m+[m[32m              label: 'Current URL',[m
[32m+[m[32m              sortable: true[m
[32m+[m[32m            },[m
[32m+[m[32m            {[m
[32m+[m[32m              key: 'formatted_url',[m
[32m+[m[32m              label: 'Formatted URL',[m
[32m+[m[32m              sortable: true[m
[32m+[m[32m            },[m
[32m+[m[32m            {[m
[32m+[m[32m              key: 'select',[m
[32m+[m[32m              label: 'Select'[m
[32m+[m[32m            }[m
[32m+[m[32m          ],[m
[32m+[m[32m          items: [],[m
[32m+[m[32m          selected: [][m
[32m+[m[32m        },[m
[32m+[m[32m        redirecttext: '',[m
[32m+[m[32m        redirectstrat: '',[m
[32m+[m[32m        stepOneComplete: false,[m
[32m+[m[32m        stepTwoComplete: false,[m
[32m+[m[32m        stepThreeComplete: false,[m
[32m+[m[32m        stepFourComplete: false,[m
[32m+[m[32m        locationComplete: false,[m
[32m+[m[32m        gmb: null,[m
[32m+[m[32m        ga: null,[m
[32m+[m[32m        strategy: null,[m
[32m+[m[32m        api_neighborhood_keywords: '',[m
[32m+[m[32m        api_landmark_keywords: '',[m
[32m+[m[32m        neighborhood_phrases: '',[m
[32m+[m[32m        landmark_phrases: '',[m
[32m+[m[32m        amenity_phrases: ''[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    updateSelect({ key, val }) {[m
[32m+[m[32m      const i = this.form.selects.findIndex(select => select.id === key)[m
[32m+[m[32m      this.form.selects[i].value = val[m
[32m+[m[32m    },[m
[32m+[m[32m    updateInput({ key, val }) {[m
[32m+[m[32m      (key === 'file')[m
[32m+[m[32m        ? this.form.inputs[key] = [val][m
[32m+[m[32m        : this.form.inputs[key] = val[m
[32m+[m[32m    },[m
[32m+[m[32m    reject(obj, keys) {[m
[32m+[m[32m      const vkeys = Object.keys(obj)[m
[32m+[m[32m        .filter(k => !keys.includes(k))[m
[32m+[m[32m      return this.pick(obj, vkeys)[m
[32m+[m[32m    },[m
[32m+[m[32m    pick(obj, keys) {[m
[32m+[m[32m      return keys.map(k => k in obj ? { [k]: obj[k] } : {})[m
[32m+[m[32m        .reduce((res, o) => Object.assign(res, o), {})[m
[32m+[m[32m    },[m
[32m+[m[32m    setMsgConfig(msg, variant, msgOn) {[m
[32m+[m[32m      this.form.msg = msg[m
[32m+[m[32m      this.form.alertvariant = variant[m
[32m+[m[32m      this.form.showMsg = msgOn[m
[32m+[m[32m    },[m
[32m+[m[32m    parseCSV(file) {[m
[32m+[m[32m      return new Promise((resolve) => {[m
[32m+[m[32m        Papa.parse(file, {[m
[32m+[m[32m          header: true,[m
[32m+[m[32m          complete: (results) => {[m
[32m+[m[32m            resolve(results.data)[m
[32m+[m[32m          }[m
[32m+[m[32m        })[m
[32m+[m[32m      })[m
[32m+[m[32m    },[m
[32m+[m[32m    getLocationData(data) {[m
[32m+[m[32m      return data[0].name ? data.map((location) => {[m
[32m+[m[32m        const { name } = location[m
[32m+[m[32m        const properties = this.reject(location, ['name'])[m
[32m+[m[32m        const addPropertyFields = this.getAddPropFields()[m
[32m+[m[32m        for (const prop in addPropertyFields) {[m
[32m+[m[32m          properties[prop] = addPropertyFields[prop][m
[32m+[m[32m        }[m
[32m+[m[32m        return { name, properties }[m
[32m+[m[32m      }).filter(location => location.name) : [][m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
