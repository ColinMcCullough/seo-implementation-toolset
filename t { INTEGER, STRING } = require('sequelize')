[32m+[m[32m        (txt) => {[m
[32m+[m[32m          return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase()[m
[32m+[m[32m        }[m
[32m+[m[32m      )[m
[32m+[m[32m    },[m
