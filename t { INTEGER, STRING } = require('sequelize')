[32m+[m[32m    validURL(str) {[m
[32m+[m[32m      const pattern = new RegExp('^(https?:\\/\\/)?' + // protocol[m
[32m+[m[32m      '((([a-z\\d]([a-z\\d-]*[a-z\\d])*)\\.)+[a-z]{2,}|' + // domain name[m
[32m+[m[32m      '((\\d{1,3}\\.){3}\\d{1,3}))' + // OR ip (v4) address[m
[32m+[m[32m      '(\\:\\d+)?(\\/[-a-z\\d%_.~+]*)*' + // port and path[m
[32m+[m[32m      '(\\?[;&a-z\\d%_.~+=-]*)?' + // query string[m
[32m+[m[32m      '(\\#[-a-z\\d_]*)?$', 'i') // fragment locator[m
[32m+[m[32m      return !!pattern.test(str)[m
[32m+[m[32m    },[m
[32m+[m[32m    // need to refactor this method[m
[32m+[m[32m    formatRedirect(redirect, strategy) {[m
[32m+[m[32m      let formatted = 'N/A'[m
[32m+[m[32m      if (strategy === 'Same Domain' && this.validURL(redirect)) {[m
[32m+[m[32m        formatted = redirect.split(/\.com\/|\.com|\.net\/|\.org\/|\.co\//)[1] // strips everthing left of the TLD (ex: www.myapartments.com/units -> units)[m
[32m+[m[32m          .split(/\.html|[?]/)[0] // strips everything to the right of .html or ?[m
[32m+[m[32m        formatted = formatted.substr(-1) === '/' ? formatted.slice(0, -1) : formatted[m
[32m+[m[32m        formatted = formatted.replace(/%20|\s/g, '\\s')[m
[32m+[m[32m          .replace(/[[\]{}()*+?.,^$|#]/g, '\\$&') // escapes special characters with \[m
[32m+[m[32m          .replace(/\/\//g, '/+') // replaces // with /+[m
[32m+[m[32m          .replace(/%5B|%5b/g, '\\[')[m
[32m+[m[32m          .replace(/%5D|%5d/g, '\\]')[m
[32m+[m[32m          .replace(/%7C/g, '\\|')[m
[32m+[m[32m        formatted = formatted.length === 1 ? `[${formatted}]` : `${formatted}$`[m
[32m+[m[32m      }[m
[32m+[m[32m      return formatted[m
[32m+[m[32m    },[m
