[32m+[m[32m            options: [[m
[32m+[m[32m              { value: null, text: 'Select Domain Strategy' },[m
[32m+[m[32m              { value: 'multi', text: 'Multi Domain' },[m
[32m+[m[32m              { value: 'single', text: 'Single Domain' }[m
[32m+[m[32m            ][m
[32m+[m[32m          },[m
[32m+[m[32m          {[m
[32m+[m[32m            id: 'branding',[m
[32m+[m[32m            value: null,[m
[32m+[m[32m            options: [[m
[32m+[m[32m              { value: null, text: 'Select Chain Branding' },[m
[32m+[m[32m              { value: 'yes', text: 'Yes' },[m
[32m+[m[32m              { value: 'no', text: 'No' }[m
[32m+[m[32m            ][m
[32m+[m[32m          }[m
[32m+[m[32m        ][m
[32m+[m[32m      },[m
[32m+[m[32m      selectedLocation: null,[m
[32m+[m[32m      locations: [],[m
[32m+[m[32m      location: {[m
[32m+[m[32m        selected: null,[m
[32m+[m[32m        options: [[m
[32m+[m[32m          { value: null, text: 'Select Location' }[m
[32m+[m[32m        ][m
[32m+[m[32m      },[m
[32m+[m[32m      tableheaders: {[m
[32m+[m[32m        fields: [[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'strategy',[m
[32m+[m[32m            label: 'Strategy'[m
[32m+[m[32m          },[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'current_url',[m
[32m+[m[32m            label: 'Current URL',[m
[32m+[m[32m            sortable: true[m
[32m+[m[32m          },[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'formatted_url',[m
[32m+[m[32m            label: 'Formatted URL',[m
[32m+[m[32m            sortable: true[m
[32m+[m[32m          },[m
[32m+[m[32m          {[m
[32m+[m[32m            key: 'wildcard',[m
[32m+[m[32m            label: 'Wildcard'[m
[32m+[m[32m          },[m
[32m+[m[32m          'remove'[m
[32m+[m[32m        ],[m
[32m+[m[32m        items: [][m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  created() {[m
[32m+[m[32m    const { lpID } = this.$nuxt._route.params[m
[32m+[m[32m    this.$axios.$get(`api/locations/${lpID}`).then((res) => {[m
[32m+[m[32m      // adds location data to front end and fills out location drop down[m
[32m+[m[32m      this.locations = res[m
[32m+[m[32m      this.location.options = [[m
[32m+[m[32m        { value: null, text: 'Select Location' },[m
[32m+[m[32m        ...res.map((location) => {[m
[32m+[m[32m          const { name, properties } = location[m
[32m+[m[32m          return { value: location.id, text: `${name} - ${properties.street_address_1}` }[m
[32m+[m[32m        })[m
[32m+[m[32m      ][m
[32m+[m[32m      this.setMsgConfig('Successfully loaded locations', 'success', true)[m
[32m+[m[32m    })[m
[32m+[m[32m  },[m
[32m+[m[32m  methods: {[m
[32m+[m[32m    loadLocation(payload) {[m
[32m+[m[32m      this.selectedLocation = this.locations.filter(location => location.id === payload)[0][m
[32m+[m[32m    },[m
[32m+[m[32m    onSave(locationID) {[m
[32m+[m[32m      // TODO validate save payload[m
[32m+[m[32m      this.$axios[m
[32m+[m[32m        .$put('api/locations', {[m
[32m+[m[32m          lpId: this.form.inputs.lpId,[m
[32m+[m[32m          locations: this.locations[m
[32m+[m[32m        })[m
[32m+[m[32m    },[m
[32m+[m[32m    onUpdate({ key, val, id }) {[m
[32m+[m[32m      const i = this.locations.findIndex(loc => loc.id === id)[m
[32m+[m[32m      if (key === 'name') {[m
[32m+[m[32m        this.locations[i][key] = val[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.locations[i].properties[key] = val[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    updateCell({ key, val, index, col, id }) {[m
[32m+[m[32m      const i = this.locations.findIndex(loc => loc.id === id)[m
[32m+[m[32m      this.locations[i].properties.redirects.items[index][col] = val[m
[32m+[m[32m    },[m
[32m+[m[32m    removeRow({ index, id }) {[m
[32m+[m[32m      const i = this.locations.findIndex(loc => loc.id === id)[m
[32m+[m[32m      this.locations[i].properties.redirects.items.splice(index, 1)[m
[32m+[m[32m    },[m
[32m+[m[32m    addRows(val, { id }) {[m
[32m+[m[32m      const i = this.locations.findIndex(loc => loc.id === id)[m
[32m+[m[32m      this.locations[i].properties.redirects.items.push(...val)[m
[32m+[m[32m    },[m
[32m+[m[32m    updateSelect({ key, val }) {[m
[32m+[m[32m      const i = this.form.selects.findIndex(select => select.id === key)[m
[32m+[m[32m      this.form.selects[i].value = val[m
[32m+[m[32m    },[m
[32m+[m[32m    updateInput({ key, val }) {[m
[32m+[m[32m      (key === 'file')[m
[32m+[m[32m        ? this.form.inputs[key] = [val][m
[32m+[m[32m        : this.form.inputs[key] = val[m
[32m+[m[32m    },[m
[32m+[m[32m    reject(obj, keys) {[m
[32m+[m[32m      const vkeys = Object.keys(obj)[m
[32m+[m[32m        .filter(k => !keys.includes(k))[m
[32m+[m[32m      return this.pick(obj, vkeys)[m
[32m+[m[32m    },[m
[32m+[m[32m    pick(obj, keys) {[m
[32m+[m[32m      return keys.map(k => k in obj ? { [k]: obj[k] } : {})[m
[32m+[m[32m        .reduce((res, o) => Object.assign(res, o), {})[m
[32m+[m[32m    },[m
[32m+[m[32m    setMsgConfig(msg, variant, msgOn) {[m
[32m+[m[32m      this.form.msg = msg[m
[32m+[m[32m      this.form.alertvariant = variant[m
[32m+[m[32m      this.form.showMsg = msgOn[m
[32m+[m[32m    },[m
[32m+[m[32m    onUpload() {[m
[32m+[m[32m      try {[m
[32m+[m[32m        Papa.parse(this.form.inputs.file, {[m
[32m+[m[32m          header: true,[m
[32m+[m[32m          complete: (res) => {[m
[32m+[m[32m            const locations = res.data.map((location) => {[m
[32m+[m[32m              const { name } = location[m
[32m+[m[32m              let properties = this.reject(location, ['name'])[m
[32m+[m[32m              properties = this.setProperties(properties)[m
[32m+[m[32m              properties.population = null[m
[32m+[m[32m              properties.uspsvalid = null[m
[32m+[m[32m              properties.recommended_name = null[m
[32m+[m[32m              properties.redirects = this.tableheaders[m
[32m+[m[32m              properties.redirecttext = ''[m
[32m+[m[32m              properties.redirectstrat = ''[m
[32m+[m[32m              return { name, properties }[m
[32m+[m[32m            })[m
[32m+[m[32m            // this.locations = locations[m
[32m+[m[32m            // writes parsed csv to database[m
[32m+[m[32m            this.$axios[m
[32m+[m[32m              .$post('api/locations', {[m
[32m+[m[32m                lpId: this.form.inputs.lpId,[m
[32m+[m[32m                locations[m
[32m+[m[32m              }).then((res) => {[m
[32m+[m[32m                // adds location data to front end and fills out location drop down[m
[32m+[m[32m                this.locations = res[m
[32m+[m[32m                this.location.options = [[m
[32m+[m[32m                  { value: null, text: 'Select Location' },[m
[32m+[m[32m                  ...res.map((location) => {[m
[32m+[m[32m                    const { name, properties } = location[m
[32m+[m[32m                    return { value: location.id, text: `${name} - ${properties.street_address_1}` }[m
[32m+[m[32m                  })[m
[32m+[m[32m                ][m
[32m+[m[32m                // this.setMsgConfig('Your CSV has been successfully imported, please select a location below', 'success', true)[m
[32m+[m[32m              })[m
[32m+[m[32m          }[m
[32m+[m[32m        })[m
[32m+[m[32m      } catch (err) {[m
[32m+[m[32m        // this.setMsgConfig('There was an error uploading the csv', 'danger', true)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
[32m+[m
