[1mdiff --git a/server/controllers/type.class.js b/server/controllers/type.class.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c4119ac[m
[1m--- /dev/null[m
[1m+++ b/server/controllers/type.class.js[m
[36m@@ -0,0 +1,105 @@[m
[32m+[m[32mconst axios = require('axios')[m
[32m+[m[32mconst diacriticMap = require('../config/diacritics')[m
[32m+[m[32mconst maps = require('./maps')[m
[32m+[m[32mclass LocationTerms {[m
[32m+[m[32m  constructor (params) {[m
[32m+[m[32m    this.class = params.class[m
[32m+[m[32m    this.vertical = params.vertical[m
[32m+[m[32m    this.typemap = null[m
[32m+[m[32m    this.neighborhoodMap = maps.neighborhoodMap[m
[32m+[m[32m    this.terms = {}[m
[32m+[m[32m    this.locations = {[m
[32m+[m[32m      type1: {},[m
[32m+[m[32m      type2: {}[m
[32m+[m[32m    }[m
[32m+[m[32m    this.lat = params.lat[m
[32m+[m[32m    this.long = params.long[m
[32m+[m[32m    this.apiKey = process.env.PLACES_API_KEY[m
[32m+[m[32m    this.city = params.city[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  async getLocationNames() {[m
[32m+[m[32m    await this.getNeighborhoodNames()[m
[32m+[m[32m    await this.getLandmarkNames()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  async getNeighborhoodNames() {[m
[32m+[m[32m    for (let i = 0; i < Object.keys(this.neighborhoodMap).length; i++) {[m
[32m+[m[32m      const key = Object.keys(this.neighborhoodMap)[i][m
[32m+[m[32m      const url = `https://maps.googleapis.com/maps/api/place/nearbysearch/json?location=${this.lat},${this.long}&types=${key}&rankby=distance&key=${this.apiKey}`[m
[32m+[m[32m      const { data } = await this.fetchTerms(url)[m
[32m+[m[32m      const resultKeys = Object.keys(data.results)[m
[32m+[m[32m      let count = resultKeys.length[m
[32m+[m[32m      if (count > this.neighborhoodMap[key]) {[m
[32m+[m[32m        count = this.neighborhoodMap[key][m
[32m+[m[32m      }[m
[32m+[m[32m      for (let x = 0; x < count; x++) {[m
[32m+[m[32m        if (!this.locations.type1[key]) {[m
[32m+[m[32m          this.locations.type1[key] = [][m
[32m+[m[32m        }[m
[32m+[m[32m        this.locations.type1[key].push(data.results[x].name)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  async getLandmarkNames() {[m
[32m+[m[32m    for (let i = 0; i < Object.keys(this.terms).length; i++) {[m
[32m+[m[32m      const key = Object.keys(this.terms)[i][m
[32m+[m[32m      const url = `https://maps.googleapis.com/maps/api/place/nearbysearch/json?location=${this.lat},${this.long}&types=${key}&radius=8000&key=${this.apiKey}`[m
[32m+[m[32m      const { data } = await this.fetchTerms(url)[m
[32m+[m[32m      const resultKeys = Object.keys(data.results)[m
[32m+[m[32m      let count = resultKeys.length[m
[32m+[m[32m      if (count > this.terms[key]) {[m
[32m+[m[32m        count = this.terms[key][m
[32m+[m[32m      }[m
[32m+[m[32m      for (let x = 0; x < count; x++) {[m
[32m+[m[32m        if (!this.locations.type2[key]) {[m
[32m+[m[32m          this.locations.type2[key] = [][m
[32m+[m[32m        }[m
[32m+[m[32m        const name = data.results[x].name[m
[32m+[m[32m        const duplicate = this.locations.type2[key].includes(name)[m
[32m+[m[32m        // const name = this.formatName(data.results[x].name)[m
[32m+[m[32m        if (!duplicate) {[m
[32m+[m[32m          this.locations.type2[key].push(name)[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  fetchTerms(url) {[m
[32m+[m[32m    // axios request here[m
[32m+[m[32m    return axios.get(url)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  formatName(name) {[m
[32m+[m[32m    this.removeDiacritics(name).replace(/[^a-zA-Z0-9\s+\-']|Helipad 1|Helipad 2|Helipad|Heliport/g, '').replace(/\-/g, ' ').replace(/\s+/g, ' ')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  removeDiacritics(name) {[m
[32m+[m[32m    for (let i = 0; i < diacriticMap.length; i++) {[m
[32m+[m[32m      name = name.replace(diacriticMap[i].letters, diacriticMap[i].base)[m
[32m+[m[32m    }[m
[32m+[m[32m    return name[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getMapName() {[m
[32m+[m[32m    const classNoSpace = this.class.replace(' ', '')[m
[32m+[m[32m    const key = `${this.vertical}${classNoSpace}Map`[m
[32m+[m[32m    this.terms = maps[key][m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  async run() {[m
[32m+[m[32m    this.getMapName()[m
[32m+[m[32m    await this.getLocationNames()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  get results() {[m
[32m+[m[32m    return this.locations[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  set results(locations) {[m
[32m+[m[32m    this.locations = locations[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = LocationTerms[m
